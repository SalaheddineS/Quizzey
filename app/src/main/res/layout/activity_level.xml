<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/_12261"
    android:gravity="center_horizontal|center_vertical"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="229dp"

        android:background="#D5000000"
        app:srcCompat="@drawable/one__9__removebg_preview" />

    <View
        android:id="@+id/divider7"
        android:layout_width="match_parent"
        android:layout_height="29dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#00FFFFFF" />

    <ImageView
        android:id="@+id/level1"
        android:layout_width="201dp"
        android:layout_height="140dp"
        android:layout_marginTop="-20dp"
        android:background="#000000"
        android:backgroundTint="#00000000"
        app:srcCompat="@drawable/peace_button_svg" />

    <View
        android:id="@+id/divider5"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#000000" />

    <ImageView
        android:id="@+id/level2"
        android:layout_width="173dp"
        android:layout_height="126dp"
        android:background="#000000"
        android:backgroundTint="#00000000"
        app:srcCompat="@drawable/yin_yang_svg" />

    <View
        android:id="@+id/divider4"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#000000"
        android:foregroundTint="#000000" />

    <ImageView
        android:id="@+id/level3"
        android:layout_width="179dp"
        android:layout_height="126dp"
        android:background="#000000"
        android:backgroundTint="#00000000"
        app:srcCompat="@drawable/_1a79246e95fb4ad3b097b5e0dbf0328_skull_bones_circle_icon" />

</LinearLayout>
