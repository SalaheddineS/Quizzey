<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/_47586"
    android:gravity="center_horizontal|center_vertical"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="229dp"

        android:background="#D5000000"
        app:srcCompat="@drawable/one__8__removebg_preview" />

    <View
        android:id="@+id/divider7"
        android:layout_width="match_parent"
        android:layout_height="29dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#00FFFFFF" />

    <ImageView
        android:id="@+id/level1"
        android:layout_width="175dp"
        android:layout_height="126dp"
        android:layout_marginTop="-20dp"
        app:srcCompat="@drawable/_97560" />

    <View
        android:id="@+id/divider5"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#00FFFFFF" />

    <ImageView
        android:id="@+id/level2"
        android:layout_width="173dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/united_kingdom_flag_icon_round_svg" />

    <View
        android:id="@+id/divider4"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#00FFFFFF" />

    <ImageView
        android:id="@+id/level3"
        android:layout_width="179dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/es_circle_01" />

</LinearLayout>
